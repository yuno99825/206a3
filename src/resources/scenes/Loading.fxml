<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #080808;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.scenes.LoadingController">
   <children>
      <Label fx:id="label" text="Loading, please wait..." textFill="#dddddd">
         <font>
            <Font size="14.0" />
         </font></Label>
      <ProgressBar fx:id="progressBar" prefWidth="300.0" progress="0.0" />
      <Button fx:id="cancelButton" maxWidth="75.0" minWidth="75.0" mnemonicParsing="false" onAction="#buttonClicked" prefWidth="75.0" text="Cancel" />
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
