<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.VideoPlayerController">
   <children>
      <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#playPauseVid" VBox.vgrow="ALWAYS" />
      <Button fx:id="restartButton" alignment="CENTER" mnemonicParsing="false" onAction="#replayButtonClicked" text="Replay" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" />
   </padding>
</VBox>
